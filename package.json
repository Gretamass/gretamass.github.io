{
    "name": "dezutis-ui",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest --environment jsdom --root src/ --mode testing",
        "test:unit-single": "vitest run --environment jsdom --root src/ --mode testing",
        "test:unit-coverage": "vitest --environment jsdom --root src/ --mode testing --coverage",
        "test:unit-coverage-single": "vitest run --environment jsdom --root src/ --mode testing --coverage",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "lint-check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --max-warnings=0",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "@fortawesome/free-brands-svg-icons": "^6.5.1",
        "@fortawesome/free-solid-svg-icons": "^6.5.1",
        "@fortawesome/vue-fontawesome": "^3.0.6",
        "@vueuse/core": "^10.9.0",
        "pinia": "^2.1.7",
        "sass": "^1.71.0",
        "vue": "^3.4.15",
        "vue-3-slider-component": "^0.1.4",
        "vue-gtag": "^2.0.1",
        "vue-router": "^4.2.5",
        "vue3-carousel": "^0.3.1"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.3.3",
        "@tsconfig/node20": "^20.1.2",
        "@types/jsdom": "^21.1.6",
        "@types/node": "^20.11.10",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.4",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "jsdom": "^24.0.0",
        "npm-run-all2": "^6.1.1",
        "prettier": "^3.0.3",
        "typescript": "~5.3.0",
        "vite": "^5.0.11",
        "vitest": "^1.2.2",
        "vue-tsc": "^1.8.27"
    }
}
